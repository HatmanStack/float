{
  "name": "audio-emotion-detection",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write '**/*.{ts,tsx,json,md}'",
    "format:check": "prettier --check '**/*.{ts,tsx,json,md}'",
    "type-check": "tsc --noEmit --skipLibCheck",
    "backend:deploy": "cd infrastructure && npm run deploy",
    "backend:validate": "cd infrastructure && npm run validate",
    "backend:logs": "cd infrastructure && npm run logs"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "./jest.setup.js"
    ],
    "setupFilesAfterEnv": [
      "./tests/unit/components/__tests__/utils/setup.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/tests/unit/components/__tests__/utils/",
      "/tests/integration/setup.ts",
      "/tests/integration/test-utils.tsx"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-google-signin/google-signin": "^14.0.1",
    "@react-navigation/native": "^6.0.2",
    "@react-oauth/google": "^0.12.2",
    "@testing-library/react-native": "^13.2.0",
    "axios": "^1.9.0",
    "expo": "~52.0.40",
    "expo-av": "^15.1.4",
    "expo-file-system": "^17.0.1",
    "expo-font": "~12.0.9",
    "expo-linking": "~6.3.1",
    "expo-notifications": "^0.31.2",
    "expo-permissions": "^14.4.0",
    "expo-router": "~3.5.21",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-web-browser": "^14.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-web": "~0.19.10"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/detox": "^18.1.3",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.45",
    "@types/react-test-renderer": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "aws-sdk": "^2.1692.0",
    "detox": "^20.46.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jest": "^29.2.1",
    "jest-expo": "^52.0.6",
    "prettier": "^3.6.2",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
  },
  "private": true
}
